{
  "expo": {
    "name": "MY-SKIN-APP",
    "slug": "MY-SKIN-APP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myskinapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "supportsTablet": true,
    "plugins": [
      "expo-web-browser"
    ],
    "android": {
      "package": "com.anonymous.MYSKINAPP"
    }
  },
  "ios": {
    "bundleIdentifier": "com.ujeen.myskinapp",
    "googleServicesFile": "./GoogleService-Info.plist"
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "googleServicesFile": "./google-services.json",
    "edgeToEdgeEnabled": true
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/splash-icon.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#F5F3EC"
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  }
}
